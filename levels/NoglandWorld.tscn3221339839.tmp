resource_name = "FPSCOUNTER"
script/source = "extends Node

var title = \"Nogland FPS Experience v0.1\"

func _ready():
	set_process(true)

func _process(_delta):
	var fps = Engine.get_frames_per_second()
	
	# Try to set window title (works on desktop, may not work in web)
	if OS.has_feature(\"web\"):
		# For web, we could use print to console instead
		print(\"FPS: \", fps)
	else:
		# For desktop builds (Godot 4 method)
		DisplayServer.window_set_title(title + \" | FPS: \" + str(fps))
	
	# Alternative: You could also create a label-based version
	# by finding a label node and updating its text:
	# var fps_label = get_node_or_null(\"../UI/FpsLabel\")
	# if fps_label:
	#     fps_label.text = \"FPS: \" + str(fps)
"

